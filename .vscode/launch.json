    "version": "0.2.0",
    "configurations": [
        {
            "name": "Cortex Debug RT1062",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/build/debug/DPU_CMake.elf",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "jlink",
            "serverpath": "/usr/bin/JLinkGDBServerCLExe",
            "serverArgs": [],
            "device": "MIMXRT1062XXX6B",
            "interface": "jtag",
            "serialNumber": "",
            "svdPath": "${workspaceFolder}/.vscode/MIMXRT1062.svd",
            "rttConfig": {
                "enabled": true,
                "address": "auto",
                "decoders": [
                    {
                        "label": "RTT stream",
                        "port": 0,
                        "type": "console"
                    }
                ]
            }
        }

    ]
    
}