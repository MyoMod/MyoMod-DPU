{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc-12 build active file",
            "command": "/usr/bin/gcc-12",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },
        {
            "type": "cmake",
            "label": "CMake: build",
            "command": "build",
            "targets": [
                "all"
            ],
            "group": "build",
            "problemMatcher": [],
            "detail": "CMake template build task"
        },
        {
            "type": "shell",
            "label": "Start RTT Logger",
            "command": "JLinkRTTLoggerExe",
            "args": [
                {
                    "value": "-If",
                    "quoting": "escape"
                 },
                 {
                     "value": "jtag",
                     "quoting": "escape"
                 },
                    {
                        "value": "-Device",
                        "quoting": "escape"
                    },
                    {
                        "value": "MIMXRT1062XXX6B",
                        "quoting": "escape"
                    },
                    {
                        "value": "-Speed",
                        "quoting": "escape"
                    },
                    {
                        "value": "4000",
                        "quoting": "escape"
                    },
                    {
                        "value": "-RTTChannel",
                        "quoting": "escape"
                    },
                    {
                        "value": "1",
                        "quoting": "escape"
                    },
                    {
                        "value": "testTask.log",
                        "quoting": "escape"
                    }
            ]
        }
    ],
    "version": "2.0.0"
}